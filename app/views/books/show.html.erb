<h2><%= @book.title %></h2>

<% if @book_photo == nil %>
  No photo available
<% else %>
  <%= image_tag @book_photo.photo.url %>
<% end %>


<ul>
  <li><%= @book.author %></li>
  <li><%= @book.publisher %></li>
  <li><%= @book.year_published %></li>
</ul>
<%= link_to 'Edit', edit_book_path(@book) %>
<%= link_to 'Delete', @book, method: :delete %>
